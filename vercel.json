{
  "builds": [
    { "src": "event_project/wsgi.py", "use": "@vercel/python", "config": { "runtime": "python3.11" } }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "event_project/wsgi.py" }
  ],
  "functions": {
    "event_project/wsgi.py": {
      "includeFiles": [
        "templates/**",
        "static/**",
        "staticfiles/**",
        "event/**",
        "users/**",
        "event_project/**"
      ]
    }
  }
}